{
  "name": "yuncms/yii2-group",
  "description": "The group extension for the yuncms",
  "keywords": [
    "yuncms",
    "group"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "xutl/yii2-fileupload-widget": "~1.0",
    "xutl/yii2-share-widget": "~1.0",
    "yuncms/yii2-user": "~2.0",
    "yuncms/yuncms-composer": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\group\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "group",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/group/messages"
      },
      "migrationNamespace": "yuncms\\group\\migrations",
      "backend": {
        "class": "yuncms\\group\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\group\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
